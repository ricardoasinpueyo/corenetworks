package Entidades;

public class Pelicula {

	private String titulo;
	private String genero;
	private Director director;
	private int	año_de_estreno;
	
	public Pelicula() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Pelicula(String titulo, String genero, Director director, int año_de_estreno) {
		super();
		this.titulo = titulo;
		this.genero = genero;
		this.director = director;
		this.año_de_estreno = año_de_estreno;
	}
	
		
	
	
	
}
